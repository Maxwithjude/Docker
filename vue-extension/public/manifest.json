{
  "name": "별담",
  "description": "북마크와 RSS 피드를 관리하는 크롬 익스텐션",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "favicon.ico"
  },
  "permissions": ["storage", "activeTab", "tabs", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {}
}
